        "module": "commonjs",
        "outDir": "./lib",
        "rootDir": "./src",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "useUnknownInCatchVariables": false,
    },
    "exclude": [
        "node_modules",
        "**/*.test.ts"
    ]
}